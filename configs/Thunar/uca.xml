<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <icon>utilities-terminal</icon>
        <name>Open Terminal Here</name>
        <unique-id>1643001293691201-1</unique-id>
        <command>wezterm start --cwd %f</command>
        <description>Open Terminal on here directory</description>
        <patterns>*</patterns>
        <startup-notify />
        <directories />
    </action>
    <action>
        <icon>nvim</icon>
        <name>Open with Neovide</name>
        <unique-id>1643001293691201-2</unique-id>
        <command>neovide %f</command>
        <description>Open File/Directory with Neovide</description>
        <patterns>*</patterns>
        <directories />
    </action>
    <action>
        <icon>edit-paste</icon>
        <name>Clip to file</name>
        <unique-id>1643001293691201-3</unique-id>
